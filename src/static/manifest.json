{
  "name": "ETH-native networks gas tracker",
  "description": "Displays the most recent gas prices of different ETH-based networks at 10-minute intervals",
  "version": "0.0.4",
  "manifest_version": 3,
  "icons": {
    "16": "images/baseIcon.png",
    "48": "images/baseIcon.png",
    "128": "images/baseIcon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Fees in Mwei",
    "default_icon": "images/baseIcon.png"
  },
  "host_permissions": [
    "https://api.etherscan.io/",
    "https://etherscan.io/"
  ],
  "permissions": [
    "alarms",
    "storage"
  ],
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
  }
}
